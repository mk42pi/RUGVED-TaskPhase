   "source": [
    "def rename_files(path, extension):\n",
    "    \n",
    "    os.chdir(path)\n",
    "    for (i,filename) in enumerate(os.listdir(path)):\n",
    "        os.rename(src=filename, dst='{}{}'.format(i,extension))"
   ]
  },
